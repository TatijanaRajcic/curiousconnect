                    <!-- ------------------------ OUR PARTNERS ------------------------ -->
                    <section class="slice-lg sct-color-1" id="partenaires">
                        <div class="sct-inner container">

                            <div class="section-title section-title--style-1 text-center mb-2">
                                <h3 class="section-title-inner transformation">
                                    <span>Les Supers Curieux</span>
                                </h3>
                                <span class="section-title-delimiter clearfix d-none"></span>
                            </div>

                            <span class="clearfix"></span>

                            <div class="fluid-paragraph fluid-paragraph-sm c-gray-light strong-300 text-center">
                                <p class="text-lg line-height-1_8">
                                    Parce qu’ils nous poussent vers le haut, un GRAND merci !
                                </p>
                            </div>

                            <span class="space-xs-xl"></span>

                            <div class="swiper-js-container">
                                <div class="swiper-container" data-swiper-items="2" data-swiper-space-between="20" data-swiper-md-items="2" data-swiper-sm-space-between="20" data-swiper-sm-items="2" data-swiper-sm-space-between="20" data-swiper-xs-items="2" data-swiper-sm-space-between="20">
                                    <div class="swiper-wrapper">
                                        <div class="swiper-slide">
                                            <div class="client-logo client-logo--style-3 img-responsive">
                                                <%= image_tag "partner1.png"%>
                                            </div>
                                        </div>
                                        <div class="swiper-slide">
                                            <div class="client-logo client-logo--style-3 img-responsive">
                                                <%= image_tag "partner2b.png"%>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                    <!-- ------------------------ END OF OUR PARTNERS ------------------------ -->

                    <!-- ------------------------ CALL TO ACTION ------------------------ -->
                    <section class="slice sct-color-4" id="contact">
                        <div class="container">
                            <div class="section-title section-title--style-1 text-center mb-2">
                                <h3 class="section-title-inner text-normal transformation" style="font-family: Raleway; color:white">
                                    Connecte-moi !
                                </h3>
                                <span class="section-title-delimiter clearfix d-none"></span>
                            </div>

                            <span class="clearfix"></span>

                            <div class="fluid-paragraph fluid-paragraph-sm c-gray-light strong-300 text-center">
                                Toujours curieux ? Bravo, tu y es presque ! Viens nous rejoindre :
                            </div>

                            <span class="space-xs-xl"></span>

                            <div class="row justify-content-center">
                                <div class="col-lg-6">
                                    <%= form_for @user, url: {action: "create"} do |f| %>
                                    <!-- Contact form -->
                                    <form id="form_contact" data-action="../../php/send-email.php" class="form-default" role="form">
                                        <div class="form-group has-feedback">
                                            <div type="text" name="name">
                                                <%= f.text_field :name, class: "form-control form-control-xl", placeholder:"Nom" %>
                                            </div>
                                            <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                                            <div class="help-block with-errors"></div>
                                        </div>

                                        <div class="form-group has-feedback">
                                            <div type="email" name="email">
                                                <%= f.text_field :email, class: "form-control form-control-xl", placeholder:"Email" %>
                                            </div>
                                            <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                                            <div class="help-block with-errors"></div>
                                        </div>

                                        <div class="form-group has-feedback">
                                            <div type="text" name="type">
                                                <%= f.select(:user_type, ["Curieux et...","Utilisateur","Partenaire"], {}, { :class => 'form-control'})  %>
                                            </div>
                                            <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                                            <div class="help-block with-errors"></div>
                                        </div>

                                        <div class="form-group has-feedback">
                                            <%= f.text_area :message, class: "form-control no-resize", rows: "5", placeholder: "Message..." %>
                                            <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                                            <div class="help-block with-errors"></div>
                                        </div>

                                        <div class="">
                                            <%= f.submit "Envoie !", class: "btn btn-block btn-styled btn-lg btn-base-2 btn-shadow strong-600 mt-4" %>
                                        </div>

                                    </form>
                                    <% end %>

                                </div>
                            </div>
                        </div>
                    </section>
                    <!-- ------------------------ END OF CALL TO ACTION ------------------------ -->


