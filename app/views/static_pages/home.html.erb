                                <div class="col-lg-4 col-md-6 col-12">

                                    <div class="icon-block--style-1-v1 block-icon-right">
                                        <%= image_tag "logo.png", class: "block-icon" %>
                                        <div class="block-content">
                                            <h3 class="heading heading-5 strong-600">Responsive web design</h3>
                                            <p class="mt-3" style="color:#818a91">
