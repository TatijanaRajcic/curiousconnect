                    <!-- ------------------------ SWIPER ------------------------ -->
                    <section class="swiper-js-container background-image-holder" data-holder-type="fullscreen" data-holder-offset=".navbar">
                        <!-- Swiper container -->
                        <div class="swiper-container" data-swiper-autoplay="true" data-swiper-effect="fade" data-swiper-items="1" data-swiper-space-between="0" data-swiper-sm-items="1" data-swiper-sm-space-between="0" data-swiper-xs-items="1" data-swiper-xs-space-between="0">
                            <!-- Swiper wrapper -->
                            <div class="swiper-wrapper">
                                <!-- Slide DISCOVER -->
                                <div class="swiper-slide" data-swiper-autoplay="2750">
                                    <div class="slice holder-item holder-item-light has-bg-cover bg-size-cover" style="background-image: url(<%= asset_path 'fond10.jpg' %>); background-position: bottom bottom">
                                        <div class="container d-flex align-items-center no-padding">
                                            <div class="col">
                                                <div class="row row-cols-xs-spaced align-items-center py-5 text-center justify-content-center">
                                                    <div class="col-lg-8">
                                                        <h3 class="heading heading-xl strong-700 c-white animated" data-animation-in="zoomIn" data-animation-onetime="false" data-animation-delay="200">
                                                            <span style="opacity: 0.9;">Le monde de demain appartient aux curieux</span>
                                                        </h3>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- Slide EXPLORE -->
                                <div class="swiper-slide" data-swiper-autoplay="2750">
                                    <div class="slice holder-item holder-item-light has-bg-cover bg-size-cover" style="background-image: url(<%= asset_path 'fond10.jpg' %>); background-position: bottom bottom">
                                        <div class="container d-flex align-items-center no-padding">
                                            <div class="col">
                                                <div class="row row-cols-xs-spaced align-items-center py-5 text-center justify-content-center">
                                                    <div class="col-lg-8">
                                                        <h3 class="heading heading-xl strong-700 c-white animated"data-animation-in="zoomIn" data-animation-onetime="false" data-animation-delay="200">
                                                            <span style="opacity: 0.9;">Curious Connect</span>
                                                        </h3>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- End of swiper wrapper -->
                            
                            <!-- Add Pagination -->
                            <div class="swiper-pagination"></div>

                        </div>
                        <!-- End of swiper container -->
                    </section>
                    <!-- ------------------------ END OF SWIPER ------------------------ -->

                    <!-- ------------------------ OUR VISION ------------------------ -->
                    <section class="slice-lg sct-color-1" id="futur">
                        <div class="container">

                            <div class="section-title section-title--style-1 text-center mb-2">
                                <h3 class="section-title-inner transformation">
                                    <span>Notre futur</span>
                                </h3>
                                <span class="section-title-delimiter clearfix d-none"></span>
                            </div>

                            <span class="clearfix"></span>

                            <div class="fluid-paragraph fluid-paragraph-sm c-gray-light strong-300 text-center">
                                <p class="text-lg line-height-1_8">
                                    Tu rêves d’un <span class = "underlined">monde qui te ressemble</span> ? Viens le créer avec ton espace d’e-learning personnalisé <span class = "underlined">Curious Connect</span>.
                                </p>
                            </div>

                            <span class="space-xs-xl"></span>

                            <div class="row cols-xs-space cols-md-space cols-sm-space">
                                <div class="col-lg-4">
                                    <div class="icon-block icon-block--style-2-v1 text-center">
                                        <%= image_tag "pbm1b.png", class: "block-icon" %>
                                        <h2 class="heading heading-4 strong-500 transformation" style="color:#E61D25">e-learning sur mesure</h2>
                                        <p class="px-4">
                                            Pour t’aider à répondre à <span class = "underlined">toutes tes questions</span>.
                                        </p>
                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="icon-block icon-block--style-2-v1 text-center">
                                        <%= image_tag "pbm2b.png", class: "block-icon" %>
                                        <h2 class="heading heading-4 strong-500 transformation" style="color:#2FBEB7">100% fun et dynamique</h2>
                                        <p class="px-4">
                                            Des <span class = "underlined">outils créatifs</span> et une app qui <span class = "underlined">évolue avec toi</span>.
                                        </p>
                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="icon-block icon-block--style-2-v1 text-center">
                                        <%= image_tag "pbm3b.png", class: "block-icon" %>
                                        <h2 class="heading heading-4 strong-500 transformation" style="color:#363390">toujours connecté</h2>
                                        <p class="px-4">
                                            <span class = "underlined">Où tu veux</span>, <span class = "underlined">quand tu veux</span>, <span class = "underlined">même sans internet</span> !
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
