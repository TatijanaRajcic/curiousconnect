                                </div>

                            </div>
                        </div>
                    </section>
                    <!-- ------------------------ END OF DEMO ------------------------ -->

                    <!-- ------------------------ OUR TEAM ------------------------ -->
                    <section class="slice-lg sct-color-4" id="team">
                        <div class="container">
                            <div class="section-title section-title--style-1 text-center mb-2">
                                <h3 class="section-title-inner transformation" style="color:white">
                                    <span>La Dream Team</span>
                                </h3>
                                <span class="section-title-delimiter clearfix d-none"></span>
                            </div>

                            <span class="clearfix"></span>

