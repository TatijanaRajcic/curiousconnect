                                    <span class="space-xs-md space-sm-xl"></span>

                                    <div class="icon-block--style-1-v1">
                                        <%= image_tag "logo.png", class: "block-icon" %>
                                        <div class="block-content">
                                            <h3 class="heading heading-5 strong-600">Online support</h3>
                                            <p class="mt-3" style="color:#818a91">
                                                Lorem ipsum dolor sit amet consectetur adipiscing elit sed do eiusmod tempor.
                                            </p>
                                        </div>
                                    </div>

                                </div>

                            </div>
                        </div>
                    </section>
                    <!-- ------------------------ END OF THE PRODUCT ------------------------ -->

                    <!-- ------------------------ DEMO ------------------------ -->
                    <section class="slice-lg sct-color-1" id="démo">
                        <div class="sct-inner container">
                            
                            <div class="section-title section-title--style-1 text-center mb-2">
                                <h3 class="section-title-inner transformation">
                                    <span>Une démo</span>
                                </h3>
                                <span class="section-title-delimiter clearfix d-none"></span>
                            </div>

                            <span class="clearfix"></span>

                            <div class="fluid-paragraph fluid-paragraph-sm c-gray-light strong-300 text-center">
                                <p class="text-lg line-height-1_8" style="color:#818a91">
                                    Coming soon !
