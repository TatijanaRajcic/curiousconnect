                                            <h3 class="heading heading-5 strong-500" style="color:white">Tatijana Rajcic</h3>
                                            <h4 class="heading heading-xs strong-400 text-uppercase letter-spacing-2 c-gray-light">Co-fondatrice et consultante créative</h4>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="block block--style-4">
                                        <div class="block block-image">
                                            <div id="essai">
                                                <%= image_tag("bianca3.png", class: "block-icon circle imageo")%>
                                                <%= image_tag("linkedin_icon.png", class: "middle", :onclick=> "window.open('https://www.linkedin.com/in/bianca-schor-443a10102')")%>
                                            </div>
                                        </div>
                                        <div class="block-body" style="text-align: center">
                                            <h3 class="heading heading-5 strong-500" style="color:white">Bianca Schor</h3>
                                            <h4 class="heading heading-xs strong-400 text-uppercase letter-spacing-2 c-gray-light">Co-fondatrice et entrepreneuse engagée</h4>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="block block--style-4">
                                        <div class="block block-image">
                                            <div id="essai">
                                                <%= image_tag("rafael4.png", class: "block-icon circle imageo")%>
                                                <%= image_tag("linkedin_icon.png", class: "middle", :onclick=> "window.open('https://www.linkedin.com/in/rafaelcoyaud/')")%>
                                            </div>
